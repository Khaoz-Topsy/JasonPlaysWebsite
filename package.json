{
  "name": "jasonplays.web",
  "version": "1.0.24",
  "scripts": {
    "build": "npm-run-all --sequential data:* generateProjectData javascript:* sass:* minifyGeneratedFiles",
    "dev": "live-server",
    "data:airtable": "node webpack/node/getAirtableData.js",
    "data:getLiveData": "node webpack/node/generateLiveJson.js",
    "generateProjectData": "node webpack/node/getGeneratedProjectJson.js",
    "minifyGeneratedFiles": "node webpack/node/minifyPublicFiles.js",
    "javascript:main": "webpack --progress --profile --mode production",
    "sass:main": "sass webpack/scss/main.scss ./assets/css/main.css"
  },
  "author": "Kurt Lourens",
  "license": "ISC",
  "devDependencies": {
    "@google-cloud/local-auth": "^1.0.1",
    "airtable": "^0.11.3",
    "axios": "^0.27.2",
    "dotenv": "^16.0.0",
    "googleapis": "^100.0.0",
    "handlebars-webpack-plugin": "^2.2.1",
    "live-server": "^1.2.1",
    "minify": "^7.2.2",
    "move-file": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "purgecss": "^4.1.3",
    "rss-parser": "^3.12.0",
    "sass": "^1.43.4",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1"
  }
}
